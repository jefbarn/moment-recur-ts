{
  "name": "moment-recur-ts",
  "version": "1.3.1",
  "description": "A momentjs plugin for matching and generating recurring dates.",
  "main": "dist/moment-recur-ts.js",
  "typings": "dist/moment-recur-ts.d.ts",
  "files": [
    "dist",
    "src",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jefbarn/moment-recur-ts.git"
  },
  "keywords": [
    "momentjs",
    "date",
    "recur"
  ],
  "author": "Jeff Barnes <jeff.barnes@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "up": "yarn upgrade-interactive --latest",
    "test": "jest",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "tslint -p . src/**/*.ts",
    "build": "yarn bundle && yarn docs",
    "bundle": "webpack --config ./webpack.config.ts",
    "docs": "typedoc src && nodetouch docs/.nojekyll",
    "clean": "rimraf dist .nyc_output",
    "prepublishOnly": "yarn build",
    "validate": "npm-run-all --parallel build test lint",
    "jest": "jest"
  },
  "peerDependencies": {
    "moment": "^2.13.0"
  },
  "devDependencies": {
    "@reactivex/ix-es2015-cjs": "^2.3.5",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/core-js": "^2.5.0",
    "@types/dts-bundle": "^0.0.30",
    "@types/jest": "^23.1.1",
    "@types/moment-timezone": "^0.5.6",
    "@types/prettier": "^1.13.1",
    "@types/rimraf": "^2.0.2",
    "@types/source-map-support": "^0.4.1",
    "@types/touch": "^3.1.0",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.2",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^2.5.7",
    "coveralls": "^3.0.1",
    "dts-bundle": "^0.7.3",
    "jest": "^23.1.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.20",
    "prettier": "^2.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.6",
    "touch": "^3.1.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.2",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "resolutions": {
    "**/moment": "2.22.2",
    "**/@types/webpack": "4.4.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "jest": {
    "transform": {
      ".ts$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "collectCoverage": true,
    "coverageDirectory": ".coverage"
  }
}
